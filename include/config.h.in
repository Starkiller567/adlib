#ifndef __config_include__
#define __config_include__

#cmakedefine HAVE_TYPEOF 1
#cmakedefine HAVE_ATTR_NONNULL 1
#cmakedefine HAVE_ATTR_WARN_UNUSED_RESULT 1
#cmakedefine HAVE_ATTR_FORMAT_PRINTF 1
#cmakedefine HAVE_ATTR_UNUSED 1
#cmakedefine HAVE_BUILTIN_EXPECT 1
#cmakedefine HAVE_MALLOC_USABLE_SIZE 1

#ifdef HAVE_TYPEOF
# define ad_typeof(x)                        __typeof__(x)
#endif

#ifdef HAVE_ATTR_NONNULL
# define _attr_nonnull(...)                  __attribute__((nonnull (__VA_ARGS__)))
#else
# define _attr_nonnull(...)
#endif

#ifdef HAVE_ATTR_WARN_UNUSED_RESULT
# define _attr_warn_unused_result            __attribute__((warn_unused_result))
#else
# define _attr_warn_unused_result
#endif

#ifdef HAVE_ATTR_FORMAT_PRINTF
# define _attr_format_printf(f, a)           __attribute__((format (printf, (f), (a))))
#else
# define _attr_format_printf(f, a)
#endif

#ifdef HAVE_ATTR_UNUSED
# define _attr_unused                        __attribute__((unused))
#else
# define _attr_unused
#endif

#ifdef HAVE_BUILTIN_EXPECT
# define likely(expr)                        __builtin_expect(!!(expr), 1)
# define unlikely(expr)                      __builtin_expect(!!(expr), 0)
#else
# define likely(expr)                        (expr)
# define unlikely(expr)                      (expr)
#endif

// array

#ifndef NDEBUG
# cmakedefine ARRAY_SAFETY_CHECKS 1
#endif

#cmakedefine ARRAY_INITIAL_SIZE              @ARRAY_INITIAL_SIZE@

#cmakedefine ARRAY_GROWTH_FACTOR_NUMERATOR   @ARRAY_GROWTH_FACTOR_NUMERATOR@
#cmakedefine ARRAY_GROWTH_FACTOR_DENOMINATOR @ARRAY_GROWTH_FACTOR_DENOMINATOR@

// dbuf

#cmakedefine DBUF_INITIAL_SIZE               @DBUF_INITIAL_SIZE@

#endif
